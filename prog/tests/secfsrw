#!/bin/sh
. Common
$LEAVE

title "Set protection to R and W, retrieve"
random_file _file
prog -w -b -5 -f - _file 2>_out <<EOF
RRRRRRRR WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW
EOF
prog -s -s >>_out
expect <<EOF
RRRRRRRR WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW
RRRRRRRR WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW
RRRRRRRR WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW
RRRRRRRR WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW WWWWWWWW
EOF
