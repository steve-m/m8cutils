#!/bin/sh
. Common

file "Protection: 2 x ( 64 x W, 64 x R ), with comments" <<EOF
; Comment starting at the beginning of the line
; (Multiple lines)
WWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWW
   ; Comment with whitespace
RRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRR  ; Comment after data
RRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRR;Without whitespace
WWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWWW
; This line has whitespace at its end   
RRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRRR

; Blank line above
EOF
asm -h -f <<EOF
EOF
security
expect <<EOF
FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF
AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA
FFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF
AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA
EOF
